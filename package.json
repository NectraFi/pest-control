  "scripts": {
    "preinstall": "node -e \"const a=(process.env.npm_config_user_agent||''); if(a.includes('npm/')){ try{ require('child_process').execSync('npx --yes npm-force-resolutions', {stdio:'inherit'});}catch(e){ console.log('npm-force-resolutions failed (continuing):', e.message);} } else { console.log('Skipping npm-force-resolutions (non-npm user agent)'); }\"",
    "publish:npm": "powershell -ExecutionPolicy Bypass -File ./scripts/publish-npm.ps1",
    "publish:npm:auto": "powershell -ExecutionPolicy Bypass -File ./scripts/publish-npm.ps1 -Token $env:NPM_TOKEN"
